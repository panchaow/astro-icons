{
  "name": "astro-icons",
  "type": "module",
  "version": "0.1.0",
  "description": "Access thousands of icons as components on-demand in your astro projects.",
  "author": "Panchao Wang <panchao626@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panchaow/astro-icons"
  },
  "keywords": [
    "astro-integration"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs"
    }
  },
  "files": [
    "./dist",
    "./types"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm --filter \"basic-example\" run dev",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "@svgr/core": "^8.1.0",
    "astro": "^3.5.3"
  },
  "peerDependenciesMeta": {
    "@svgr/core": {
      "optional": true
    }
  },
  "dependencies": {
    "@iconify/types": "^2.0.0",
    "@iconify/utils": "^2.1.11",
    "effect": "2.0.0-next.55",
    "local-pkg": "^0.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.2",
    "@changesets/cli": "^2.26.2",
    "@svgr/core": "^8.1.0",
    "@types/node": "^20.9.0",
    "astro": "^3.5.3",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.5.0"
  }
}
